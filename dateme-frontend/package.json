{
  "name": "dateme-frontend",
  "scripts": {
    "dev": "NODE_ENV=development NODE_SRC=\".\" node server.js",
    "build": "NODE_SRC=\".\" next build",
    "start": "NODE_ENV=production NODE_SRC=\".\" node server.js",
    "prod-status": "pm2 deploy production exec \"cd dateme-frontend; pm2 l\"",
    "logs":
      "ssh -t root@165.227.58.224 \"cd /root/applytodate/current/dateme-frontend; pm2 logs\"",
    "deploy": "pm2 deploy ecosystem.config.js production --force",
    "postdeploy":
      "yarn build && pm2 startOrRestart ecosystem.config.js --env production"
  },
  "babel": {
    "presets": ["next/babel"],
    "plugins": [
      [
        "inline-react-svg",
        {
          "svgo": {
            "plugins": [
              {
                "removeAttrs": {
                  "attrs": ["(data-name)", "(width)", "(height)"]
                }
              },
              {
                "removeDesc": true
              },
              {
                "removeUselessDefs": true
              },
              {
                "cleanupIDs": true
              },
              {
                "convertColors": true
              },
              {
                "removeTitle": true
              },
              {
                "removeMetadata": true
              },
              {
                "cleanupAttrs": true
              },
              {
                "removeUselessStrokeAndFill": true
              },
              {
                "mergePaths": true
              },
              {
                "collapseGroups": true
              }
            ]
          }
        }
      ]
    ]
  },
  "dependencies": {
    "@zeit/next-css": "^0.0.7",
    "@zeit/next-less": "^0.0.9",
    "babel-plugin-inline-react-svg": "^0.5.2",
    "bluebird": "^3.5.1",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.9",
    "dotenv": "^5.0.0",
    "dotenv-webpack": "^1.5.4",
    "express": "^4.16.2",
    "invariant": "^2.2.2",
    "is-social": "^1.1.1",
    "less": "^2.7.3",
    "lodash": "^4.17.5",
    "mobile-detect": "^1.4.1",
    "next": "^5.0.0",
    "next-redux-wrapper": "^1.3.5",
    "next-routes": "^1.2.0",
    "postcss-loader": "^2.1.0",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "raven-js": "^3.22.1",
    "rc-switch": "^1.6.0",
    "react": "^16.2.0",
    "react-burger-menu": "^2.2.3",
    "react-cropper": "^1.0.1",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.7",
    "react-ga": "^2.4.1",
    "react-hamburger-menu": "^1.0.0",
    "react-image-crop": "^3.0.10",
    "react-images": "^0.5.16",
    "react-markdown": "^3.1.5",
    "react-onclickoutside": "^6.7.1",
    "react-places-autocomplete": "^6.1.1",
    "react-portal": "^4.1.2",
    "react-redux": "^5.0.6",
    "react-responsive-modal": "^2.0.0",
    "react-router": "^4.2.0",
    "react-s-alert": "^1.4.1",
    "react-s3-uploader": "^4.6.2",
    "react-social-login": "^3.4.2",
    "react-spinkit": "^3.0.0",
    "react-stickynode": "^1.4.1",
    "react-typed": "^1.0.8",
    "react-waypoint": "^8.0.0",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "slideout": "^1.0.1",
    "style-loader": "^0.20.1",
    "superagent": "^3.8.2",
    "superagent-jsonapify": "^1.4.5",
    "title-case": "^2.1.1"
  },
  "devDependencies": {
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "extract-text-webpack-plugin": "^3.0.2"
  }
}
