{
  "name": "dateme-frontend",
  "scripts": {
    "dev": "NODE_ENV=development NODE_SRC=\".\" node server.js",
    "build": "NODE_SRC=\".\" next build",
    "start": "NODE_ENV=production NODE_SRC=\".\" node server.js"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "inline-react-svg",
        {
          "svgo": {
            "plugins": [
              {
                "removeAttrs": {
                  "attrs": "(data-name)"
                }
              },
              {
                "cleanupIDs": true
              }
            ]
          }
        }
      ]
    ]
  },
  "dependencies": {
    "@zeit/next-css": "^0.0.7",
    "@zeit/next-less": "^0.0.9",
    "bluebird": "^3.5.1",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.9",
    "dotenv": "^5.0.0",
    "dotenv-webpack": "^1.5.4",
    "express": "^4.16.2",
    "less": "^2.7.3",
    "lodash": "^4.17.5",
    "next": "^5.0.0",
    "next-redux-wrapper": "^1.3.5",
    "next-routes": "^1.2.0",
    "postcss-loader": "^2.1.0",
    "raven-js": "^3.22.1",
    "react": "^16.2.0",
    "react-cropper": "^1.0.1",
    "react-dom": "^16.2.0",
    "react-ga": "^2.4.1",
    "react-image-crop": "^3.0.10",
    "react-images": "^0.5.16",
    "react-markdown": "^3.1.5",
    "react-redux": "^5.0.6",
    "react-responsive-modal": "^2.0.0",
    "react-s-alert": "^1.4.1",
    "react-social-login": "^3.4.2",
    "react-spinkit": "^3.0.0",
    "react-stickynode": "^1.4.1",
    "react-waypoint": "^8.0.0",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.20.1",
    "superagent": "^3.8.2",
    "superagent-jsonapify": "^1.4.5",
    "title-case": "^2.1.1"
  },
  "devDependencies": {
    "babel-plugin-inline-react-svg": "^0.5.2",
    "extract-text-webpack-plugin": "^3.0.2"
  }
}
